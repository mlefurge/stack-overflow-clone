<% if @errors %>
  <ul><% @errors.each do |error| %>
    <li><%= error %></li>
  <% end %></ul>
<% end %>

<div class="new_question_container">
  <%= form_for @question do |f| %>
    <%= render partial: "form", locals: {f: f, question: @question } %>
    <%= f.submit "Edit Question" %>
  <% end %>
</div>
